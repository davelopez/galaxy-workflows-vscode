{
  "compilerOptions": {
    "target": "es2019",
    "lib": ["ES2019", "WebWorker"],
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "composite": true,
    "paths": {
      "@schemas/*": ["./workflow-languages/schemas/*"]
    }
  },
  "exclude": []
}
